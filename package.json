{
  "name": "vue-dashboard",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "lint-staged": "lint-staged",
    "prepare": "husky",
    "update-api": "npx mushroom-cli update ts-code"
  },
  "lint-staged": {
    "**/*.{js,ts,vue}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "element-plus": "^2.11.9",
    "mushroomjs": "1.2.0-alpha.5",
    "mushroomjs-auth": "^1.1.0",
    "mushroomjs-file": "^1.2.0",
    "sweetalert2": "^11.26.3",
    "vue": "^3.5.25",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@tsconfig/node22": "^22.0.5",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "9.39.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.6.2",
    "husky": "^9.1.7",
    "jiti": "^2.5.1",
    "lint-staged": "^16.2.7",
    "mushroom-cli": "^1.2.1",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.7.3",
    "sass": "^1.94.2",
    "typescript": "~5.9.0",
    "vite": "^7.2.4",
    "vue-tsc": "^3.1.5"
  }
}
